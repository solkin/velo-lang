type IntPair struct(
  int a,
  int b,
  str c
); # компилируется в функцию, создающую структуру - по сути, конструктор типа

auto p = IntPair(5, 7, "string"); # вызывается функция, которая возвращает кастомный тип данных intPair

println(p.c);
println(p.a + p.b); # организация доступа к именованным props структур
