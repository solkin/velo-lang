native class Terminal() {
    native func print(str text) void;
    native func input() str;
    func println(str text) void {
        print(text.con("\n"));
    };
};
native class Time() {
    native func sleep(int millis) void;
    native func print(Terminal term) void;
};

Terminal term = new Terminal();
Time time = new Time();
time.sleep(1000);
time.print(term);
