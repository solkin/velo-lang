fn[int] test = func(int a) int {
    func(int a, int b) int {
        a + b
    }(a, 1);
};

func traverse(slice[slice[int]] matrix) slice[slice[int]] {
    matrix.map(
        func(int index, slice[int] row) slice[int] {
            row.map(
                func(int index, int col) int {
                    int v = test(col);
                    print(v);
                    print(" ");
                    v;
                }
            );
            println("");
            row;
        }
    );
};

slice[slice[int]] matrix = sliceOf[slice[int]](
    sliceOf[int](0, 4, 6, 3, 5),
    sliceOf[int](8, 4, 2, 5, 1)
);

traverse(matrix);
